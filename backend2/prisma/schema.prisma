// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}
model Created{
  id String @id
  name String
  description String
  createdAt DateTime @default(now())
  @@map("created")
}
model Deleted{
  id String @id
  name String
  description String
  createdAt DateTime @default(now())
  @@map("deleted")
}
